# ApplicationContext Initializers
org.springframework.context.ApplicationContextInitializer=\
com.yangbingdong.spring.explore.lifecycle.app.CustomApplicationContextInitializer

# Run Listeners
org.springframework.boot.SpringApplicationRunListener=\
com.yangbingdong.spring.explore.lifecycle.app.CustomSpringApplicationRunListener

