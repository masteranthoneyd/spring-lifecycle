org.springframework.context.ApplicationListener=\
com.yangbingdong.spring.explore.lifecycle.app.SpringApplicationEventListener.ApplicationStartingEventListener,\
com.yangbingdong.spring.explore.lifecycle.app.SpringApplicationEventListener.ApplicationEnvironmentPreparedEventListener,\
com.yangbingdong.spring.explore.lifecycle.app.SpringApplicationEventListener.ApplicationContextInitializedEventListener,\
com.yangbingdong.spring.explore.lifecycle.app.SpringApplicationEventListener.ApplicationPreparedEventListener
